{
  "name": "Default Anime Character",
  "version": "1.0.0",
  "author": "AI Video Creator",
  "description": "Default anime character template for video generation",
  "resolution": {
    "width": 512,
    "height": 512
  },
  "layers": {
    "background": "background.png",
    "character": "character.png",
    "mouth_open": "mouth_open.png",
    "mouth_closed": "mouth_closed.png",
    "eyes_open": "eyes_open.png",
    "eyes_closed": "eyes_closed.png"
